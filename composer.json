{
    "name": "coliving/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/dungnh/api-guard",
    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6|^7.4|^8.0|^8.1",
        "illuminate/support": "^5.4 | ^6.0 | ^7.0 | ^8.0 | ^9.0",
        "illuminate/database": "^5.4 | ^6.0| ^7.0 | ^8.0 | ^9.0",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Chrisbjr\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "stable"
}
